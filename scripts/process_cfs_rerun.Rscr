## rerun cfsv2 realtime in the past

raw_dir = '/d2/anewman/wrf_hydro/cfsv2'
script_dir = '/home/hydrofcst/s2s/scripts'

dates = list.dirs(raw_dir, full.names = FALSE, recursive = FALSE)
dates = dates[8:512] #remove non dates

for (i in 1:length(dates)) {
  date_i = dates[i]
  print(paste0('running ',date_i))
  system(paste0(script_dir,'/process_cfs_realtime.csh ',date_i))
}
